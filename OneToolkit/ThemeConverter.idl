namespace OneToolkit.UI.Theming
{
	enum AbsoluteTheme
	{
		Light = 0,
		Dark = 1
	};

	enum RelativeTheme
	{
		Default = 0,
		Light = 1,
		Dark = 2
	};

    [default_interface]
	runtimeclass ThemeConverter
	{
		[noexcept] static AbsoluteTheme ToAbsoluteTheme(RelativeTheme value, AbsoluteTheme defaultTheme);
    }
}